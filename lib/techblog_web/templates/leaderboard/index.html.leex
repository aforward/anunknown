<div class="block-article">
  <div class="container">
    <table class="leaderboard">
      <tr>
        <th>Name</th>
        <th>Gender</th>
        <th>Age Category</th>
        <th class="sortable-column" phx-click="sort_by" phx-value-sort="all">Open Score</th>
        <th class="sortable-column" phx-click="sort_by" phx-value-sort="20.1">20.1 Score</th>
        <th class="sortable-column" phx-click="sort_by" phx-value-sort="20.2">20.2 Score</th>
      </tr>
      <%= for {athlete,data} <- @leaderboard do %>
        <tr>
          <td><%= athlete %></td>
          <td><%= data.gender %></td>
          <td><%= data.age_category %></td>
          <td><%= data.open20x.position %> (<%= data.open20x.summary %>)</td>
          <td><%= data.open201.position %> (<%= data.open201.summary %>)<%= if data.open201.mode != "rx", do: " #{data.open201.mode}" %></td>
          <td><%= data.open202.position %> (<%= data.open202.summary %>)<%= if data.open202.mode != "rx", do: " #{data.open201.mode}" %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
<%= render TechblogWeb.LayoutView, "spinner.html", assigns %>



